{
  "mcpServers": {
    "mimic": {
      "command": "docker",
      "args": ["run", "--rm", "-i", "-e", "MODE=mcp", "-e", "GITHUB_TOKEN", "-e", "UNIFY_API_KEY", "cloudbeesdemo/mimic:latest"],
      "env": {
        "GITHUB_TOKEN": "your_github_token_here",
        "UNIFY_API_KEY": "your_unify_api_key_here"
      }
    }
  }
}